{
  "name": "create-rcade",
  "type": "module",
  "version": "0.0.9",
  "description": "A CLI tool to generate a new rcade project",
  "repository": {
    "type": "git",
    "url": "https://github.com/fcjr/RCade"
  },
  "bin": {
    "create-rcade": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build index.ts --outdir dist --target node --banner '#!/usr/bin/env node'"
  },
  "dependencies": {
    "rcade": "workspace:*"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
